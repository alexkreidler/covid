@article{neuzil_ultra_2006,
	title = {Ultra fast miniaturized real-time {PCR}: 40 cycles in less than six minutes},
	volume = {34},
	issn = {0305-1048},
	shorttitle = {Ultra fast miniaturized real-time {PCR}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC1904101/},
	doi = {10.1093/nar/gkl416},
	abstract = {We have designed, fabricated and tested a real-time PCR chip capable of conducting one thermal cycle in 8.5 s. This corresponds to 40 cycles of PCR in 5 min and 40 s. The PCR system was made of silicon micromachined into the shape of a cantilever terminated with a disc. The thin film heater and a temperature sensor were placed on the disc perimeter. Due to the system's thermal constant of 0.27 s, we have achieved a heating rate of 175°C s−1 and a cooling rate of −125°C s−1. A PCR sample encapsulated with mineral oil was dispensed onto a glass cover slip placed on the silicon disc. The PCR cycle time was then determined by heat transfer through the glass, which took only 0.5 s. A real-time PCR sample with a volume of 100 nl was tested using a FAM probe. As the single PCR device occupied an area of only a few square millimeters, devices could be combined into a parallel system to increase throughput.},
	number = {11},
	urldate = {2020-03-21},
	journal = {Nucleic Acids Research},
	author = {Neuzil, Pavel and Zhang, Chunyan and Pipper, Juergen and Oh, Sharon and Zhuo, Lang},
	year = {2006},
	pmid = {16807313},
	pmcid = {PMC1904101},
	pages = {e77},
	file = {PubMed Central Full Text PDF:/home/alex/snap/zotero-snap/common/Zotero/storage/7B7SCIZS/Neuzil et al. - 2006 - Ultra fast miniaturized real-time PCR 40 cycles i.pdf:application/pdf}
}

@article{santos_development_2017,
	title = {Development, validation and testing costs of an in-house real-time {PCR} assay for the detection of {Chlamydia} trachomatis},
	volume = {66},
	issn = {0022-2615,},
	url = {https://www.microbiologyresearch.org/content/journal/jmm/10.1099/jmm.0.000443},
	doi = {10.1099/jmm.0.000443},
	abstract = {{\textless}span class="jp-bold"{\textgreater}Purpose.{\textless}/span{\textgreater} To improve the screening of {\textless}span class="jp-italic"{\textgreater}Chlamydia trachomatis{\textless}/span{\textgreater}  
               {\textless}span class="jp-italic"{\textgreater}(C. trachomatis){\textless}/span{\textgreater} in Brazil, an accurate and affordable method is needed. The objective of this study was to develop and assess the performance and costs of a new in-house real-time PCR (qPCR) assay for the diagnosis of {\textless}span class="jp-italic"{\textgreater}C. trachomatis{\textless}/span{\textgreater} infection.  
             
               {\textless}span class="jp-bold"{\textgreater}Methodology.{\textless}/span{\textgreater} Asymptomatic women aged 14–25 years who attended primary health services in Manaus, Brazil, were screened for {\textless}span class="jp-italic"{\textgreater}C. trachomatis{\textless}/span{\textgreater} using the Digene Hybrid Capture II CT-ID (HCII CT-ID) DNA test. A subset of cervical specimens were tested using an in-house qPCR and a commercial qPCR, {\textless}span class="jp-italic"{\textgreater}Artus{\textless}/span{\textgreater}  
               {\textless}span class="jp-italic"{\textgreater}C. trachomatis{\textless}/span{\textgreater} Plus RG PCR 96 CE (Artus qPCR) kit, as a reference test. A primer/probe based on the sequence of cryptic plasmid (CP) was designed. An economic evaluation was conducted from the provider’s perspective.  
             
               {\textless}span class="jp-bold"{\textgreater}Results.{\textless}/span{\textgreater} The primers were considered specific for {\textless}span class="jp-italic"{\textgreater}C. trachomatis{\textless}/span{\textgreater} because they did not amplify any product from non-sexually transmitted bacterial species tested. Overall, 292 specimens were tested by both the commercial kit (Artus qPCR) and the in-house qPCR. Of those, one resulted in no amplification and was excluded from the analysis. The sensitivity, specificity, and positive and negative predictive values of the in-house qPCR were 99.5 \% [95 \% confidence interval (CI): 97.1–100], 95.1 \% (95 \% CI: 89–98.4), 97.4 \% (95 \% CI: 94–99.1) and 99.0 \% (95 \% CI: 94.5–100), respectively. The cost per case of {\textless}span class="jp-italic"{\textgreater}C. trachomatis{\textless}/span{\textgreater} was £0.44 (\$0.55) for HCII CT-ID, £1.16 (\$1.45) for Artus qPCR and £1.06 (\$1.33) for in-house qPCR.  
             
               {\textless}span class="jp-bold"{\textgreater}Conclusion.{\textless}/span{\textgreater} We have standardized an in-house qPCR to detect cervical {\textless}span class="jp-italic"{\textgreater}C. trachomatis{\textless}/span{\textgreater} targeting CP. The in-house qPCR showed excellent accuracy and was more affordable than the commercial qPCR kit.,},
