	pmcid = {PMC6291133},
	file = {PubMed Central Full Text PDF:/home/alex/snap/zotero-snap/common/Zotero/storage/BUCYUQPG/Tambo et al. - 2018 - Loop-mediated isothermal amplification (LAMP) and .pdf:application/pdf}
}

@article{vasquez_diagnostic_2018,
